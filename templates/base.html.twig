            <link rel="stylesheet" href="{{ asset('css/dash.css') }}">
            <link rel="stylesheet" href="{{ asset('css/style.css') }}">
            <link rel="stylesheet" href="{{ asset('css/main.css') }}">
            <link rel="stylesheet" href="{{ asset('css/structure.css') }}">
        {% endblock %}
        {% block javascripts %}
